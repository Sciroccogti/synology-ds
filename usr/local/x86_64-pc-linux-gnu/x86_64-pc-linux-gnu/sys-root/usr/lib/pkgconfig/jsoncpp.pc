prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: jsoncpp
Description: jsoncpp core libs
Version: 1.0
Libs: -L${libdir} -ljson
Cflags: -I${includedir} -DSYNO_ARRAY_SEARCH -DSYNO_CONVERT_PATCH -DSYNO_WRITE_PATCH -DSYNO_READ_PATCH -DSYNO_DSM_6_0 -DSYNO_ASSERT_PATCH
